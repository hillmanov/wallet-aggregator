{
  "name": "Wallet Aggregator",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://wallet.google.com/merchant/pages/*"],
      "css": ["css/styles.css"],
      "js": ["js/lib.js", "js/app.js", "js/templates.js"]
    }
  ],
  "web_accessible_resources": [
    "templates/*"
  ]
}
